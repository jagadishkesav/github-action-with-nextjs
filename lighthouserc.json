{
    "ci": {
      "collect": {
        "url": [
          " https://jagadishkesav.github.io/github-action-with-nextjs/test"
        ],
        "startServerCommand": "http-server ./build/client -p 4000 -g",
        "startServerReadyPattern": "Available on",
        "numberOfRuns": 1
      },
      "upload": {
        "target": "temporary-public-storage"
      },
      "assert": {
        "preset": "lighthouse:recommended",
        "assertions": {
            "first-contentful-paint": ["error", { "minScore": 0.6 }]
          }
      }
    }
  }